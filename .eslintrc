{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["react"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "error",
        "quotes": ["error", "double"],
        "semi": "error"
    }
}
